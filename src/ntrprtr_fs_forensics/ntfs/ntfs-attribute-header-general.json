{
    "name": "NTFS MFT Attribute Header Examiner",
    "description": "Examines the General Part of NTFS MFT Attribute Headers",
    "ntrprtr": [
        {
            "name": "typeId",
            "description": "Type Identifier",
            "start": 0,
            "end": 3,
            "action": [
                {
                    "type": "endianess",
                    "endianess": "little"
                },
                {
                    "type": "equals",
                    "endianess": "little",
                    "cmp": [
                        {
                            "value": "00000010",
                            "description": "$STANDARD_INFORMATION"
                        },
                        {
                            "value": "00000020",
                            "description": "$ATTRIBUTE_LIST"
                        },
                        {
                            "value": "00000030",
                            "description": "$FILE_NAME"
                        },
                        {
                            "value": "00000040",
                            "description": "$OBJECT_ID"
                        },
                        {
                            "value": "00000050",
                            "description": "$SECURITY_DESCRIPTOR"
                        },
                        {
                            "value": "00000060",
                            "description": "$VOLUME_NAME"
                        },
                        {
                            "value": "00000070",
                            "description": "$VOLUME_INFORMATION"
                        },
                        {
                            "value": "00000080",
                            "description": "$DATA"
                        },
                        {
                            "value": "00000090",
                            "description": "$INDEX_ROOT"
                        },
                        {
                            "value": "000000A0",
                            "description": "$INDEX_ALLOCATION"
                        },
                        {
                            "value": "000000B0",
                            "description": "$BITMAP"
                        },
                        {
                            "value": "000000C0",
                            "description": "$REPARSE_POINT"
                        },
                        {
                            "value": "000000D0",
                            "description": "$EA_INFORMATION"
                        },
                        {
                            "value": "000000E0",
                            "description": "$EA"
                        },
                        {
                            "value": "00000100",
                            "description": "$LOGGED_UTILITY_STREAM"
                        }
                    ],
                    "noMatch": "Attribute Type unknown!"
                }
            ]
        },
        {
            "name": "attributeLength",
            "description": "Attribute Length",
            "start": 4,
            "end": 7,
            "action": [
                {
                    "type": "endianess",
                    "endianess": "little"
                },
                {
                    "type": "decimal",
                    "endianess": "little"
                }
            ]
        },
        {
            "name": "formCode",
            "description": "Form Code",
            "start": 8,
            "end": 8,
            "action": [
                {
                    "type": "equals",
                    "endianess": "big",
                    "noMatch": "Resident/Non Resident unknown!",
                    "cmp": [
                        {
                            "value": "00",
                            "description": "Resident Attribute"
                        },
                        {
                            "value": "01",
                            "description": "Non Resident Attribute"
                        }
                    ]
                }
            ]
        },
        {
            "name": "nameLen",
            "description": "Name Length",
            "start": 9,
            "end": 9,
            "action": [
                {
                    "type": "endianess",
                    "endianess": "little"
                },
                {
                    "type": "decimal",
                    "endianess": "little"
                }
            ]
        },
        {
            "name": "nameOffset",
            "description": "Name Offset",
            "start": 10,
            "end": 11,
            "action": [
                {
                    "type": "endianess",
                    "endianess": "little"
                },
                {
                    "type": "decimal",
                    "endianess": "little"
                }
            ]
        },
        {
            "name": "flags",
            "description": "Flags",
            "start": 12,
            "end": 13,
            "action": []
        },
        {
            "name": "attrId",
            "description": "Attribute Identifier",
            "start": 14,
            "end": 15,
            "action": []
        }
    ]
}